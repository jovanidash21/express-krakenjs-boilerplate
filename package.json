{
  "name": "express-krakenjs-boilerplate",
  "version": "0.1.0",
  "description": "Node.js Express Kraken.js - Application",
  "author": "Jovani Warguez",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "build": "concurrently \"npm run build:client\" \"npm run build:server\"",
    "build:client": "grunt build",
    "build:server": "cross-env NODE_ENV=production node server.js",
    "dev": "concurrently -k \"grunt watch\" \"nodemon server.js --watch build\""
  },
  "dependencies": {
    "concurrently": "^3.5.0",
    "construx": "^1.0.0",
    "construx-copier": "^1.0.0",
    "construx-dustjs": "^1.1.0",
    "dust-makara-helpers": "^4.1.2",
    "construx-sass": "^1.0.0",
    "eslint": "^1.10.3",
    "express": "^4.12.2",
    "kraken-js": "^2.0.0",
    "makara": "^2.0.3"
  },
  "devDependencies": {
    "bower": "^1.8.0",
    "browser-sync": "^2.18.13",
    "cross-env": "^5.0.3",
    "grunt": "^0.4.5",
    "grunt-cli": "^0.1.13",
    "grunt-config-dir": "^0.3.2",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-copy-browser-modules": "^6.0.0",
    "grunt-copy-to": "0.0.10",
    "grunt-dustjs": "^1.4.0",
    "grunt-eslint": "^17.3.2",
    "grunt-mocha-cli": "^1.14.0",
    "mocha": "^1.18.0",
    "grunt-sass": "^1.2.1",
    "nodemon": "^1.11.0",
    "supertest": "^0.9.0"
  }
}
